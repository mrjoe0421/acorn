
public class Testclass {

	public static void main(String[] args) {
		
		float result = 9/2f;
		int result_int= 9/2;
		System.out.println(result);
		System.out.println(result_int);
		char c ='A';
		int int_c = c+1;
		char c2 = (char)(c+1);
		System.out.println(result);
		System.out.println(c+ "\t" + int_c + "\c" + c2);
	}

}
