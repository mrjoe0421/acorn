
public class MyTest01 {

	public static void main(String[] args) {
		
		String name = "홍길동";
		String address = "서울";
		int age = 20;
		
		System.out.println("이름은 " + name );
		System.out.println("나이는 " + age + "이고 주소는 " + address);
		
		System.out.print("이름은" + name);		
		System.out.print("\n나이는" + age + "이고 주소는" + address);		
			
				

	}

}
