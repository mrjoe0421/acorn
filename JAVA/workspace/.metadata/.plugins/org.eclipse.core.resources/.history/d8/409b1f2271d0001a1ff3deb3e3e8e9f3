
public class OperaterTest_01 {

	public static void main(String[] args) {
		
		int a = 3;
		++a ;
		System.out.println(a);
		a--;
		System.out.println(a);
		
		int x = 5;
		int y = ++x;
		System.out.println(x + " " + y);
		
		int x2 = 5;
		int y2 = x2++;
		System.out.println(x2 + " " + y2);

	}

}
