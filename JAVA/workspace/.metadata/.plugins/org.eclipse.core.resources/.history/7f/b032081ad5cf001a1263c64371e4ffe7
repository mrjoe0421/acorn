
public class MyTest03 {

	public static void main(String[] args) {
		int n = 10;
		int n2 = 20;
		
		//값 변경 코드
		int temp = n;
		n = n2;
		n2 = temp;
		
		int tomp = n2;
		n = n2;
		n2 = tomp;

		
		System.out.println(" n값은 " + temp);
		System.out.println("n2값은 " + tomp);
	}

}
