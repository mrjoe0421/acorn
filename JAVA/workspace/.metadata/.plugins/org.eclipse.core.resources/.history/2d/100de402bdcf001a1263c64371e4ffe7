
public class Sample03_typeCasting {

	public static void main(String[] args) {
		//데이터 타입 변경(자동으로 변경된다0
		//1. byte - > short - > int ->long->float->double (작은 -->큰)
		byte b = 10;
		short b2 = b;
		int b3 = b2;
		long b4 = b3;
		float b5 = b4;
		double b6 = b5;
		
		//2. char --> int
		char n = 'A';
		int n2 = n+1;
		System.out.println(n2);
		
		//3. int보다 작은 타입의 연산결과는 int로
		short x = 10;

	}

}
