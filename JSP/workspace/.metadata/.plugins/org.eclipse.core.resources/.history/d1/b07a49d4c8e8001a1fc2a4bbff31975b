<script type="text/javascript">
	var num = 10; //전역변수
	function outputNumbers(count){
		for(var i = 0; i < count; i++){
			console.log(i);
		}
		console.log(">>" + i); //5
		console.log(">>" + num); //5
	}
	
	outputNumbers(5);
	/* 
	자바나 C++에서 
	*/
</script>
