<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
<script type="text/javascript">
//a와 b의 배수가 아닌 수의 총합
	function test(a,b){
		
		var sum = 0;
		for(var i = 0; i<=20; i++){
			if(i%a!=0 %% i%b==0){
				sum+=i;
			}			

		}
		return sum;
	}


	
	var a = prompt("숫자를 입력하시오.");
	var b = prompt("숫자를 입력하시오.");
	a = parseInt(a);
	b = parseInt(b);
	
	console.log("총합 : " + test(a, b));
</script>
</head>
