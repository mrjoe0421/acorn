	//1. 연결 연산자로 동작됨.
	 var x1 = "10" + 5;  //주의 
	 var x2 = "10" - 5;
	 var x3 = "10" * 5;
	 var x4 = "10" / 5;
	 
	 console.log(x1+" " + x2 + "  " + x3 + " " + x4);
	
	 
	 //2. 불린 값
	 var y1 = 0;
	 var y2 = '';
	 var name="hello";
	 var obj= [];
	 var newName= new String("hello");
	 
	 if(newName){
		 console.log("newName는 true이다.");
	 }
	 if(obj){
		 console.log("obj는 true이다.");
	 }
	 if(name){
		 console.log("hello는 true이다");
	 }
	 if(!y1){
		 console.log("y1은 false이다"+y1);
	 }
	 if(!y2){
		 console.log("y2은 false이다"+y2);
	 }
	 
	 
</script>

