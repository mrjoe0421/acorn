	var k4 = ("22") ? 100 : 200;// true 5가지 false값이 아니라서
	console.log(k+"\t"+k2 + "\t" + k3 + "\t" + k4);
	document.write(k + "&nbsp;&nbsp;&nbsp;&nbsp;" + k2 +"&nbsp;&nbsp;&nbsp;&nbsp;" + k3 + "&nbsp;&nbsp;&nbsp;&nbsp;" + k4 +"&nbsp;&nbsp;&nbsp;&nbsp;");
